{
  "name":"pai-cthulhu/adminbereich",
  "type": "project",
  "description":"Microframework simples para uso em sites dinÃ¢micos",
  "keywords": [
    "framework"
  ],
  "license": "Apache-2.0",
  "authors":[
    {
      "name":"William J. Venancio",
      "email":"william.jvenancio@gmail.com"
    }
  ],
  "support": {
    "email": "william.jvenancio@gmail.com"
  },
  "autoload":{
    "psr-4":{
      "AdmBereich\\": "library/",
      "abApp\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require":{
    "php": ">=7.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "eftec/bladeone": "^3",
    "kint-php/kint": "^3",
    "robmorgan/phinx": "^0.11.3",
    "scssphp/scssphp": "^1",
    "vlucas/phpdotenv": "^4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v0.6.0-dev"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
